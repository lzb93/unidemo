.navs-dinwei {
  position: absolute;
  width: 100%;
  top: 0;
}
.navs-dinwei image {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: 15rpx;
    top: 12rpx;
}
.navs-dinwei .main-navul {
    position: absolute;
    z-index: 999;
    color: #333;
    width: 200rpx;
    top: 80rpx;
    font-size: 15px;
    border-left: 1px solid #f8f8f8;
    border-right: 1px solid #f8f8f8;
    right: 10rpx;
}
.navs-dinwei .main-navul .main-navli {
      width: 200rpx;
      background: #fff;
      border-bottom: 1px solid #f8f8f8;
      height: 80rpx;
      line-height: 80rpx;
}
.title {
  color: #8f8f94;
  margin-top: 50rpx;
}
.ul {
  font-size: 30rpx;
  color: #8f8f94;
  margin-top: 50rpx;
}
.ul > view {
  line-height: 50rpx;
}
.maintop-bg {
  background: -webkit-gradient(linear, left bottom, left top, from(#9bd8f7), to(#0faeff));
  background: -o-linear-gradient(bottom, #9bd8f7, #0faeff 100%);
  background: linear-gradient(0deg, #9bd8f7, #0faeff 100%);
  height: 240rpx;
}
.topbox {
  border: 1px solid #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  width: 80%;
  border-right: 8rpx;
  margin: auto;
}
.topli {
  border-right: 1px solid #fff;
  text-align: center;
  width: 25%;
  color: #fff;
  line-height: 40rpx;
  font-size: 24rpx;
}
.qiun-charts {
  width: 750rpx;
  height: 500rpx;
  background-color: #FFFFFF;
}
.charts {
  width: 750rpx;
  height: 500rpx;
  background-color: #FFFFFF;
}

